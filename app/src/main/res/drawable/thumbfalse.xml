<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/white" />
            <size
                android:width="8dp"
                android:height="8dp" />
            <stroke
                android:width="1dp"
                android:color="@color/tiffany_blue" />
        </shape>
    </item>
    <item
        android:bottom="8dp"
        android:drawable="@drawable/sun"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp" />
</layer-list>